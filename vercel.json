{
  "framework": "vite",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "outputDirectory": "f1-data-viz/frontend/dist",
  "installCommand": "npm install",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.js" },
    {
      "source": "/(.*\\.(js|css|svg|ico|jpg|jpeg|png|webp|avif|gif|woff|woff2|ttf|otf)$)",
      "destination": "/$1"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
