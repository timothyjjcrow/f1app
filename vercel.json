{
  "framework": "vite",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "outputDirectory": "f1-data-viz/frontend/dist",
  "installCommand": "npm install",
  "rewrites": [{ "source": "/api/(.*)", "destination": "/api/index.js" }],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*).js", "dest": "/$1.js" },
    { "src": "/(.*).css", "dest": "/$1.css" },
    { "src": "/(.*).svg", "dest": "/$1.svg" },
    { "src": "/(.*).ico", "dest": "/$1.ico" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
