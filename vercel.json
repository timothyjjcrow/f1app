{
  "version": 2,
  "builds": [
    {
      "src": "f1-data-viz/frontend/package.json",
      "use": "@vercel/vite"
    },
    {
      "src": "f1-data-viz/backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "f1-data-viz/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "f1-data-viz/frontend/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "f1-data-viz/frontend/index.html"
    }
  ]
}
